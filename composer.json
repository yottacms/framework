{
    "name": "yottacms/framework",
    "description": "Basic framework for Yotta.CMS",
    "keywords": [
        "yottacms",
        "symfony",
        "dependencies",
        "framework"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandr Hrischanovich",
            "email": "info@asdf.by"
        }
    ],
    "require": {
        "php": ">=7.2",
        "symfony/http-kernel": "^4|^5"
    },
    "require-dev": {
        "symfony/dependency-injection": "^4.1.12|^5",
		"symfony/config": "^4|^5",
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "YottaCms\\Framework\\": ""
        },
		"exclude-from-classmap": [
            "Tests/"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
